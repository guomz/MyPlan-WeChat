<view class="page">
  <view class="head-title">
    <image src='{{userInfo.avatarUrl}}' mode='cover' class='avatar'></image>
    <view class="page__title">昵称</view>
    <view class="page__desc">{{userInfo.nickName}}</view>
  </view>

  <view class='page__bd'>
    <view class="weui-cells__title">添加计划</view>
    <view class="weui-cells weui-cells_after-title">
      <navigator url="/pages/addPlan/addPlan?userId={{userId}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view class="weui-cell__hd">
          <image src="/images/icons/edit.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
        </view>
        <view class="weui-cell__bd">Add Plan</view>
        <view class="weui-cell__ft weui-cell__ft_in-access">去编辑</view>
      </navigator>
    </view>

    <view class="weui-cells__title">我的日历</view>
    <view class="weui-cells weui-cells_after-title">
      <navigator url="/pages/myCalendar/myCalendar" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view class="weui-cell__hd">
          <image src="/images/icons/mycalendar.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
        </view>
        <view class="weui-cell__bd">My Calendar</view>
        <view class="weui-cell__ft weui-cell__ft_in-access">去查看</view>
      </navigator>
    </view>
  </view>

  <!-- <button bindtap='onTap'>下载测试</button> -->

</view>